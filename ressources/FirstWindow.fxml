<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="350.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ihm.FirstWindowController">
   <children>
      <HBox alignment="BOTTOM_CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nombres de couleurs" textAlignment="CENTER" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="118.0" prefWidth="297.0" VBox.vgrow="ALWAYS">
         <children>
            <ComboBox fx:id="firstWinCombobox" maxHeight="50.0" maxWidth="200.0" prefWidth="150.0" style="-fx-alignment: center;" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="firstWinButton" maxHeight="50.0" maxWidth="200.0" mnemonicParsing="false" text="Confirm" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
</VBox>
